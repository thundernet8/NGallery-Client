{
  "presets": ["react", "es2015", "es2017", "stage-2"],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }],
    "add-module-exports",
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "~src/styles/global/theme"
      }
    ]]],
  "comments": false
}
